                ItemName="ItemsFromFile"/>
        </ReadLinesFromFile>
    <!--<Exec CommAND="$(SolutionDir)ResourceEmbedder\bin\$(ConfigurationName)\ResourceEmbedder.exe %22ViridianLink.dll%22 %22$(TargetPath)%22 %22$(TargetName)%22 false" />-->
  </Target>
  <Target Name="TestTarget2">
	<Message Text="A" Condition=" '$(SettingCounter)'=='1' AND HasTrailingSlash(%(ItemsFromFile.Identity))"/>
	<CreateProperty
            Value="%(ItemsFromFile.Identity)"
			Condition=" '$(SettingCounter)'=='1' AND HasTrailingSlash(%(ItemsFromFile.Identity)) AND '$(HollowKnightLocation)' == 'NULL'">
            <Output
                TaskParameter="Value"
                PropertyName="HollowKnightLocation" />
    </CreateProperty>
	<Message Text="B" Condition=" '$(SettingCounter)'=='1'  AND '$(HollowKnightLocation)' != 'NULL'"/>
	<CreateProperty
            Value="2"
			Condition=" '$(SettingCounter)'=='1' AND '$(HollowKnightLocation)' != 'NULL'">
            <Output
                TaskParameter="Value"
                PropertyName="SettingCounter" />
    </CreateProperty>
	
	
	<Message Text="C" Condition=" '$(SettingCounter)'=='2' AND HasTrailingSlash(%(ItemsFromFile.Identity)) AND '%(ItemsFromFile.Identity)'!='$(HollowKnightLocation)' "/>
	<CreateProperty
            Value="%(ItemsFromFile.Identity)"
			Condition=" '$(SettingCounter)'=='2' AND HasTrailingSlash(%(ItemsFromFile.Identity)) AND '%(ItemsFromFile.Identity)'!='$(HollowKnightLocation)' AND '$(EditorProjectLocation)' == 'NULL'">
            <Output
                TaskParameter="Value"
                PropertyName="EditorProjectLocation" />
    </CreateProperty>
	<Message Text="D" Condition=" '$(SettingCounter)'=='2' AND '$(EditorProjectLocation)' != 'NULL'"/>
	<CreateProperty
            Value="3"
			Condition=" '$(SettingCounter)'=='2' AND '$(EditorProjectLocation)' != 'NULL'">
            <Output
                TaskParameter="Value"
                PropertyName="SettingCounter" />
    </CreateProperty>
	
	
	<Message Text="E" Condition=" '$(SettingCounter)'=='3' AND HasTrailingSlash(%(ItemsFromFile.Identity)) AND '%(ItemsFromFile.Identity)'!='$(HollowKnightLocation)' AND '%(ItemsFromFile.Identity)'!='$(EditorProjectLocation)'"/>
	<CreateProperty
            Value="%(ItemsFromFile.Identity)"
			Condition=" '$(SettingCounter)'=='3' AND HasTrailingSlash(%(ItemsFromFile.Identity)) AND '%(ItemsFromFile.Identity)'!='$(HollowKnightLocation)' AND '%(ItemsFromFile.Identity)'!='$(EditorProjectLocation)' AND '$(UnityEditorLocation)' == 'NULL'">
            <Output
                TaskParameter="Value"
                PropertyName="UnityEditorLocation" />
    </CreateProperty>
	<Message Text="F" Condition=" '$(SettingCounter)'=='3' AND $(UnityEditorLocation) != 'NULL'"/>
	<CreateProperty
            Value="4"
			Condition=" '$(SettingCounter)'=='3' AND '$(UnityEditorLocation)' != 'NULL'">
            <Output
                TaskParameter="Value"
                PropertyName="SettingCounter" />
    </CreateProperty>
